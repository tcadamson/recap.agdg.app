{% extends "base.html.jinja" %}

{% import "macros.jinja" as macros %}

{% block title %}{{datestamp | semantic_datestamp}}{% endblock title %}

{% block content %}
<h1 class="title">{{self.title()}}</h1>
<div class="catalog">
    {% for title in rows | map(attribute = "title") | unique %}
    {{macros.catalog_entry_common(rows, title)}}
    {% endfor %}
</div>
{% endblock content %}
