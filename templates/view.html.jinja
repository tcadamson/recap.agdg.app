{% extends "base.html.jinja" %}

{% import "macros.jinja" as macros %}

{% block title %}{{datestamp | semantic_datestamp}}{% endblock title %}

{% block content %}
<h1>{{self.title()}}</h1>
<div class="catalog">
    {% for title in rows | map(attribute = "title") %}
    {{macros.catalog_entry(rows | selectattr("title", "==", title), title, url_for("game", title = title)) | indent}}
    {% endfor %}
</div>
{% endblock content %}
